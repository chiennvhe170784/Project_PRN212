<Window x:Class="ProjectPRN.History1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectPRN"
        mc:Ignorable="d"
        Title="History" Height="450" Width="800">
    <Grid>

        <DataGrid x:Name="dataHistory" AutoGenerateColumns="False"  Margin="126,22,284,122">
          
            <DataGrid.Columns>
                <DataGridTextColumn Header="Code Name" Binding="{Binding Code}" Width="*" />
                <DataGridTextColumn Header="Mark" Binding="{Binding Mark}" Width="*"/>
                <DataGridTextColumn Header="Date" Binding="{Binding Date}" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Return" HorizontalAlignment="Left" Margin="218,353,0,0" VerticalAlignment="Top" Height="27" Width="109" Click="Button_Click"/>

    </Grid>
</Window>
