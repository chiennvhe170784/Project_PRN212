<Window x:Class="ProjectPRN.InputCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectPRN"
        mc:Ignorable="d"
        Title="InputCode" Height="400" Width="600">
    <Grid>
        <Label Content="Enter exam code" HorizontalAlignment="Left" Margin="121,99,0,0" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="txtCodeE" HorizontalAlignment="Left" Margin="284,99,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="190" Height="31"/>
        <Button Content="Join Exam" HorizontalAlignment="Left" Margin="284,164,0,0" VerticalAlignment="Top" Height="24" Width="94" Click="Button_Join_Click"/>
        <Button Content="View History" HorizontalAlignment="Left" Margin="132,232,0,0" VerticalAlignment="Top" Height="24" Width="94" Click="Button_History_Click"/>
        <Button Content="Quit" HorizontalAlignment="Left" Margin="132,276,0,0" VerticalAlignment="Top" Height="24" Width="94" Click="Button_Quit_Click" />
        <TextBlock HorizontalAlignment="Left" Margin="249,36,0,0" TextWrapping="Wrap" Text="Student Exam" VerticalAlignment="Top" Height="32" Width="164" Foreground="#FFB51C1C" FontSize="20"/>

    </Grid>
</Window>
